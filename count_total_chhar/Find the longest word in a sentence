#include <stdio.h>

int main(void) {
    char s[2001];
    if (fgets(s, sizeof(s), stdin)) {
        int maxLen = 0, cur = 0;
        char* longest = NULL;
        char* start = NULL;

        for (char* p = s; ; ++p) {
            char c = *p;
            if (c != ' ' && c != '\t' && c != '\n' && c != '\0') {
                if (cur == 0) start = p;
                ++cur;
            } else {
                if (cur > maxLen) {
                    maxLen = cur;
                    longest = start;
                }
                cur = 0;
                if (c == '\0') break;
            }
        }
        // print longest word
        if (longest && maxLen > 0) {
            for (int i = 0; i < maxLen; ++i) putchar(longest[i]);
        }
        putchar('\n');
    }
    return 0;
}
